<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tinh.appdautien">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/messenger"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/messenger"
        android:supportsRtl="true"
        android:theme="@style/Theme.Appdautien">

        <activity android:name=".raletive_layout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Appdautien.NoActionBar">

        </activity>
    </application>

</manifest>